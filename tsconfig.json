{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },

  "include": [
    "src"
  ],

  "resolve": {
    "extensions": [
      ".ts",
      ".tsx",
      ".js",
      ".jsx",
      ".json",
      ".png"
    ]
  }
}
